            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
          </g>
          <g class="g--circle">
            <use xlink:href="#s--circle" class="u--circle"/>
