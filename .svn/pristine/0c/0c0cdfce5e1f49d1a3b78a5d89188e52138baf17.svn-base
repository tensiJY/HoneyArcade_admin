.btn-brown {
    color: #fff !important;
    background-color: #43200A !important;
    border-color: #43200A;
}		
.btn-brown:hover {
    color: #fff;
    background-color: #43200A;
    border-color: #43200A;
}
/* 파일 업로드 공통*/
.fileUpload {
    position: relative;
    overflow: hidden;
    margin: 10px;
}
.fileUpload input.upload {
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;
    padding: 0;
    font-size: 20px;
    cursor: pointer;
    opacity: 0;
    filter: alpha(opacity=0);
}
/*체크박스*/
input[type=checkbox]{
	zoom:1.4;
}
/* display*/
.display-none{
	display:none;
}
.display-show{
	display:show;
}
.lcateSelect {
	border: 1px sienna solid;
}