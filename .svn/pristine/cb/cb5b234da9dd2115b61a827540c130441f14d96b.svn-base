														<th:block th:if="${dongList != null}">
															<th:block th:if="${dongList.size() != 0}">
																<th:block th:each="list : ${dongList}">
																	<option th:value="${list.dong_seq}"
																		th:text="${list.dong_name}"
																		th:selected="${ownerInfo.dong_seq == list.dong_seq}">
																	</option>
																</th:block>
															</th:block>
														</th:block>
													</select>
