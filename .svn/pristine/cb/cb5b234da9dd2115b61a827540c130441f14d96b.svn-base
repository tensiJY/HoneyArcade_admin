																<option value=3>월~토</option>
																<option value=4>월</option>
																<option value=5>화</option>
																<option value=6>수</option>
																<option value=7>목</option>
																<option value=8>금</option>
																<option value=9>토</option>
																<option value=10>일</option>
																<option value=11>해당사항없음</option>
															</select>
															<label class="mr-2 mb-2">오픈시간</label>
															<input type="text" id="breakDayStart"
																class="form-control mr-4 mb-2"
																style="vertical-align: middle;" placeholder="09:00"
																readonly>
															<label class="mr-2 mb-2">마감시간</label>
															<input type="text" id="breakDayEnd"
																class="form-control mr-4 mb-2"
																style="vertical-align: middle;" placeholder="18:00"
