													<div class="form-row mb-2">
														<div class="col-3">
															<input type="text" id="member_id" name="member_id"
																class="form-control disabled" placeholder="honeyArcade"
																th:value="${ownerInfo.member_id}" readonly>
															<input type="hidden" id="owner_id" name="owner_id" th:value="${ownerInfo.owner_id}">																
														</div>
														<div class="col">
															<input type="text" id="owner_email" name="owner_email"
																class="form-control" placeholder="honeyArcade@naver.com"
																th:value="${ownerInfo.owner_email}" readonly>
														</div>
														<div class="col">
															<input type="text" id="owner_phone" name="owner_phone"
																class="form-control" placeholder="010-1234-1234"
																th:value="${ownerInfo.owner_phone}" readonly>
														</div>
													</div>
													<select class="selectpicker" data-live-search="true" title="시,도"
														id="sidoSelect" name="sido_seq" data-width="fit" disabled>
														<th:block th:if="${sidoList.size()!=0}">
															<th:block th:each="list : ${sidoList}">
																<option th:value="${list.sido_seq}"
																	th:text="${list.sido_name}"
																	th:selected="${ownerInfo.sido_seq == list.sido_seq}">
																</option>
															</th:block>
														</th:block>
													</select>
													<select class="selectpicker" data-live-search="true" title="군,구"
														id="sigunguSelect" name="sigungu_seq" data-width="fit" disabled>
														<th:block th:if="${sigunguList != null}">
															<th:block th:if="${sigunguList.size() != 0}">
																<!-- 값이 있는경우 -->
																<th:block th:each="list : ${sigunguList}">
																	<option th:value="${list.sigungu_seq}"
																		th:text="${list.sigungu_name}"
																		th:selected="${ownerInfo.sigungu_seq == list.sigungu_seq}">
																	</option>
																</th:block>
															</th:block>
														</th:block>
													</select>
													<select class="selectpicker" data-width="fit"
														data-live-search="true" id="dongSelect" name="dong_seq"
