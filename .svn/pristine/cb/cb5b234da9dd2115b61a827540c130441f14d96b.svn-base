																		th:selected="${ownerInfo.store_floor == list.floor_seq}">
																	</option>
																</th:block>
															</th:block>
														</th:block>
													</select>
													<div class="form-row">
														<div class="col-3">
															<input type="text" id="store_name" name="store_name"
