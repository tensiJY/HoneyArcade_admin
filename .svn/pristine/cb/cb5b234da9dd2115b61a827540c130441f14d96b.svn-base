	<link rel="stylesheet" type="text/css" th:href="@{/resources/cork/plugins/flatpickr/flatpickr.css}">
	<link rel="stylesheet" type="text/css" th:href="@{/resources/honeyArcade/admin/common/css/common.css}">
	<link rel="stylesheet" type="text/css" th:href="@{/resources/honeyArcade/admin/common/viewerjs/css/viewer.css}">

	<!--  css style  -->
