															<th:block th:if="${buildList.size() != 0}">
																<th:block th:each="list : ${buildList}">
																	<option th:value="${list.build_seq}"
																		th:text="${list.build_name}"
																		th:selected="${ownerInfo.build_seq == list.build_seq}">
																	</option>
																</th:block>
															</th:block>
														</th:block>
													</select>
