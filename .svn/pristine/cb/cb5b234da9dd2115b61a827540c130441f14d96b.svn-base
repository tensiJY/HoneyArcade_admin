																		readonly>
																	<input type="button"
																		class="btn btn-danger mr-3 breakDayDelBtn"
																		value="삭제">
																	<input type="hidden" name="break_day"
																		th:value="${list.break_day}">
																	<input type="hidden" name="open_time"
																		th:value="${list.open_time}">
																	<input type="hidden" name="close_time"
