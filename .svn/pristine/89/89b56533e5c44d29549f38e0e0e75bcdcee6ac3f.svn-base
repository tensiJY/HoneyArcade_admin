									<input type="hidden" name="dong_seq" th:value="${AdminMgtBuildVO.dong_seq}">
									<input type="hidden" name="search_text" th:value="${AdminMgtBuildVO.search_text}">
								</th:block>
								<th:block th:if="${dtlType == 2}"></th:block>
							</form>
							<div id="delFileSeq">
							</div>
						</div>
					</div>
				</div>

				<!-- CONTENT AREA -->
			</div>
			<!--  footer   -->
			<th:block th:replace="fragments/footer"></th:block>
		</div>
