	//	승인 - 컬백
	function _acceptCallback(res){
		if(res.result == false || res.result == 'false'){
			return alert("승인 문제가 생겼습니다. 관리자에게 문의 바랍니다.");
		}
		alert("승인 처리하였습니다");
		window.location.reload();
	}
	
