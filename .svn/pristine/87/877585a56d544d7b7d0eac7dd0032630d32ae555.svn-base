<!DOCTYPE html>
<html lang="ko" xmlns:th="http://www.thymeleaf.org" xmlns:sec="http://www.thymeleaf.org/thymeleaf-extras-springsecurity5" >
<head>
	<th:block th:replace="fragments/meta :: metaFragment(title='점주 웹 이미지')"></th:block>
		
	<!--  css style  -->
	<th:block th:replace="fragments/style :: styleFragment"></th:block>
</head>


<body class="sidebar-noneoverflow">
    
	<!--  header  -->
	<th:block th:replace="fragments/header :: headerFragment"></th:block>
	

    <!--  BEGIN MAIN CONTAINER  -->
    <div class="main-container" id="container">

        <div class="overlay"></div>
        <div class="search-overlay"></div>

		<!--  sidebar  -->
		<th:block th:replace="fragments/sidebar :: sidebarFragment(main='set', sub='set-web')"></th:block>
		        
        <!--  BEGIN CONTENT AREA  -->
        <div id="content" class="main-content">
            <div class="layout-px-spacing">


                <!-- CONTENT AREA -->
                <div class="row layout-top-spacing">
                    
					<div id="" class="col-8 layout-spacing">
						<div class="statbox widget box box-shadow">
							<div class="widget-header">
								<div>
									<div class="col-xl-12 col-md-12 col-sm-12 col-12">
										<h4>공지사항 입력</h4>
									</div>
								</div>
							</div>
							<div class="widget-content widget-content-area">
								<div class="col-12">
									<input type="text" class="form-control col-6" style="vertical-align: middle;" placeholder="제목">
									<textarea type="text" class="form-control mt-2" rows="20" placeholder="내용 입력"></textarea>
									<div class="mt-4 float-right mb-4">
										<button class="btn btn-primary mr-1">저장</button>
										<button class="btn btn-danger mr-1">삭제</button>
										<button class="btn btn-dark">목록</button>
									</div>
								</div>
							</div>
						</div>
					</div>
					
                </div>


                <!-- CONTENT AREA -->

            </div>
           
            <!--  footer   -->
            <th:block th:replace="fragments/footer"></th:block>
			
            
        </div>
        <!--  END CONTENT AREA  -->

    </div>
    <!-- END MAIN CONTAINER -->

	<th:block th:replace="fragments/script :: scriptFragment"></th:block>
	
	<script th:inline="javascript">
	/* <![CDATA[ */
		
	/*]] */
	</script>
</body>

</html>